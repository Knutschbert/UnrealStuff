from __future__ import annotations
from typing import Any, List, Dict, Optional, overload, TypeVar, Generic, Tuple
import enum, System, CUE4Parse
T = TypeVar('T')
class Lazy(Generic[T]): value: T
Nullable = Optional

class ACLException(System.Exception, System.Runtime.Serialization.ISerializable):
    def __init__(self, message: 'str') -> None: ...

class ACLNative:
    LIB_NAME: 'str' = ...
    @staticmethod
    def nAllocate(size: 'int', alignment: 'int') -> 'System.IntPtr': ...
    @staticmethod
    def nDeallocate(ptr: 'System.IntPtr', size: 'int') -> None: ...

class RawBufferHeader(System.ValueType):
    Size: 'System.UInt32' = ...
    Hash: 'System.UInt32' = ...

class TracksHeader(System.ValueType):
    Tag: 'System.UInt32' = ...
    Version: 'System.UInt16' = ...
    AlgorithmType: 'int' = ...
    TrackType: 'int' = ...
    NumTracks: 'System.UInt32' = ...
    NumSamples: 'System.UInt32' = ...
    SampleRate: 'float' = ...
    MiscPacked: 'System.UInt32' = ...
    def GetHasScale(self) -> 'bool': ...
    def GetDefaultScale(self) -> 'int': ...

class CompressedTracks:
    @overload
    def __init__(self, buffer: 'List[int]') -> None: ...
    @overload
    def __init__(self, existing: 'System.IntPtr') -> None: ...
    Handle: 'System.IntPtr' = ...
    def IsValid(self, checkHash: 'bool') -> 'str': ...
    def GetTracksHeader(self) -> 'CUE4Parse.ACL.TracksHeader': ...
    def SetDefaultScale(self, scale: 'System.UInt32') -> None: ...

