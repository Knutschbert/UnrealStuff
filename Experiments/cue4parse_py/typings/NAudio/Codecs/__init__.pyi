class ALawDecoder:
    def __init__(self) -> None: ...
    @staticmethod
    def ALawToLinearSample(aLaw: 'int') -> 'System.Int16': ...

class ALawEncoder:
    @staticmethod
    def LinearToALawSample(sample: 'System.Int16') -> 'int': ...

class G722Codec:
    def __init__(self) -> None: ...
    def Decode(self, state: 'NAudio.Codecs.G722CodecState', outputBuffer: 'List[System.Int16]', inputG722Data: 'List[int]', inputLength: 'int') -> 'int': ...
    def Encode(self, state: 'NAudio.Codecs.G722CodecState', outputBuffer: 'List[int]', inputBuffer: 'List[System.Int16]', inputBufferCount: 'int') -> 'int': ...

class G722CodecState:
    def __init__(self, rate: 'int', options: 'NAudio.Codecs.G722Flags') -> None: ...
    ItuTestMode: 'bool' = ...
    Packed: 'bool' = ...
    EncodeFrom8000Hz: 'bool' = ...
    BitsPerSample: 'int' = ...
    QmfSignalHistory: 'List[int]' = ...
    Band: 'List[NAudio.Codecs.Band]' = ...
    InBuffer: 'System.UInt32' = ...
    InBits: 'int' = ...
    OutBuffer: 'System.UInt32' = ...
    OutBits: 'int' = ...

class Band:
    def __init__(self) -> None: ...
    s: 'int' = ...
    sp: 'int' = ...
    sz: 'int' = ...
    r: 'List[int]' = ...
    a: 'List[int]' = ...
    ap: 'List[int]' = ...
    p: 'List[int]' = ...
    d: 'List[int]' = ...
    b: 'List[int]' = ...
    bp: 'List[int]' = ...
    sg: 'List[int]' = ...
    nb: 'int' = ...
